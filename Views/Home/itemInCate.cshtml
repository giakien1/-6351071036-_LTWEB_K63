@model IEnumerable<BookStore.Models.SACH>
@{
    ViewBag.Title = "itemInCate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3 style="text-align:center" ; color="red">Sach theo chu de</h3>
<div>
    @foreach (var item in Model)
    {
        <a href="@Url.Action("Details", "Home", new {id = item.Masach})"><img src="~/image/@item.Anhbia" alt="@item.Tensach" style="width:100px;height:auto;"></a>
        
        <div>
            <br />
            <h4>@item.Tensach</h4>

        </div>
    }
</div>

